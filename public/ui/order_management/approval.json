{
  "ui": {
    "layout": "",
    "root": [
      {
        "tag": "div",
        "classes": "px-4 sm:px-6 lg:px-8 border py-4 rounder-lg",
        "children": [
          {
            "tag": "div",
            "classes": "sm:flex sm:items-center",
            "children": [
              {
                "tag": "div",
                "classes": "sm:flex-auto",
                "children": [
                  {
                    "tag": "h1",
                    "classes": "text-base font-semibold leading-6 text-gray-900",
                    "text": "Users"
                  },
                  {
                    "tag": "p",
                    "classes": "mt-2 text-sm text-gray-700",
                    "text": "A list of all the users in your account including their name, title, email, and role."
                  }
                ]
              },
              {
                "tag": "div",
                "classes": "mt-4 sm:ml-16 sm:mt-0 sm:flex-none flex justify-end",
                "children": [
                  {
                    "tag": "button",
                    "type": "button",
                    "classes": "block rounded-md bg-indigo-600 px-3 py-2 text-center text-sm font-semibold text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600 ",
                    "text": "Add user"
                  }
                ]
              }
            ]
          },
          {
            "tag": "div",
            "classes": "mt-8 flow-root",
            "children": [
              {
                "tag": "div",
                "classes": "-mx-4 -my-2 overflow-x-auto sm:-mx-6 lg:-mx-8",
                "children": [
                  {
                    "tag": "div",
                    "classes": "inline-block min-w-full py-2 align-middle sm:px-6 lg:px-8",
                    "children": [
                      {
                        "tag": "table",
                        "classes": "min-w-full divide-y divide-gray-300",
                        "children": [
                          {
                            "tag": "thead",
                            "children": [
                              {
                                "tag": "tr",
                                "children": [
                                  {
                                    "tag": "th",
                                    "scope": "col",
                                    "classes": "py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-0",
                                    "text": "Name"
                                  },
                                  {
                                    "tag": "th",
                                    "scope": "col",
                                    "classes": "px-3 py-3.5 text-left text-sm font-semibold text-gray-900",
                                    "text": "Title"
                                  },
                                  {
                                    "tag": "th",
                                    "scope": "col",
                                    "classes": "px-3 py-3.5 text-left text-sm font-semibold text-gray-900",
                                    "text": "Status"
                                  },
                                  {
                                    "tag": "th",
                                    "scope": "col",
                                    "classes": "px-3 py-3.5 text-left text-sm font-semibold text-gray-900",
                                    "text": "Role"
                                  },
                                  {
                                    "tag": "th",
                                    "scope": "col",
                                    "classes": "relative py-3.5 pl-3 pr-4 sm:pr-0",
                                    "children": [
                                      {
                                        "tag": "span",
                                        "classes": "sr-only",
                                        "text": "Edit"
                                      }
                                    ]
                                  }
                                ]
                              }
                            ]
                          },
                          {
                            "tag": "tbody",
                            
                            "children":[
                              {
                                "tag":"tr",
                                "children":[
                                  {
                                    "tag":"td",
                                    "api": "apidata"
                                   
                                  }
                             
                                ]
                              }

                            ]
                           
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    
    ]
  }
}
